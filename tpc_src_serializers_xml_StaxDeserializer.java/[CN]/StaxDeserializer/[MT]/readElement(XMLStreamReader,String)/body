{
  if (parser.getEventType() != XMLStreamConstants.START_ELEMENT) {
    while (parser.next() != XMLStreamConstants.START_ELEMENT) {
    }
  }
  while (true) {
    if (parser.getLocalName().equals(string)) {
      return parser.getElementText();
    }
    while (parser.next() != XMLStreamConstants.START_ELEMENT) {
    }
  }
}
