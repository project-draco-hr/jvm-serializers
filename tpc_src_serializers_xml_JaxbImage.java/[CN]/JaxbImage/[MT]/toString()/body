{
  StringBuilder sb=new StringBuilder();
  sb.append("[Image ");
  sb.append("uri=").append(repr(uri));
  sb.append(", title=").append(repr(title));
  sb.append(", width=").append(width);
  sb.append(", height=").append(height);
  sb.append(", size=").append(size);
  sb.append("]");
  return sb.toString();
}
