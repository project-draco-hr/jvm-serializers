{
  SerializeWriter writer=new SerializeWriter(new OutputStreamWriter(out,"UTF-8"),JSON.DEFAULT_GENERATE_FEATURE,SerializerFeature.WriteEnumUsingToString,SerializerFeature.DisableCircularReferenceDetect);
  try {
    for (int i=0, len=items.length; i < len; ++i) {
      JSON.writeJSONString(items[i],out,SerializerFeature.WriteEnumUsingToString,SerializerFeature.DisableCircularReferenceDetect);
    }
  }
  finally {
    writer.close();
  }
}
