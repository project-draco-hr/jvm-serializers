{
  if (bean.f_uri != null) {
    out.writeBoolean(true);
    out.writeUTF(bean.f_uri);
  }
 else {
    out.writeBoolean(false);
  }
  if (bean.f_title != null) {
    out.writeBoolean(true);
    out.writeUTF(bean.f_title);
  }
 else {
    out.writeBoolean(false);
  }
  out.writeInt(bean.f_width);
  out.writeInt(bean.f_height);
  if (bean.f_size != null) {
    out.writeBoolean(true);
    out.writeShort(bean.f_size.getNumber());
  }
 else {
    out.writeBoolean(false);
  }
}
