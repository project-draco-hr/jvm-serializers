{
  _media=null;
  if (in.readBoolean()) {
    _media=new Media();
    _media.readExternal(in);
  }
  _images=null;
  int nbImages=in.readInt();
  if (nbImages >= 0) {
    _images=new ArrayList<Image>(nbImages);
    for (int i=0; i < nbImages; i++) {
      Image image=new Image();
      image.readExternal(in);
      _images.add(image);
    }
  }
}
