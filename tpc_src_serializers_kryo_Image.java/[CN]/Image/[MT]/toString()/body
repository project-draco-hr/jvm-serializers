{
  StringBuilder sb=new StringBuilder();
  sb.append("[Image ");
  sb.append("width=").append(_width);
  sb.append(", height=").append(_height);
  sb.append(", uri=").append(_uri);
  sb.append(", title=").append(_title);
  sb.append(", size=").append(_size);
  sb.append("]");
  return sb.toString();
}
