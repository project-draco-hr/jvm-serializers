{
  parser.nextToken();
  parser.nextToken();
  Media media=new Media();
  media.setPlayer(Media.Player.valueOf(readStringElement(parser,"pl")));
  media.setUri(readStringElement(parser,"ul"));
  media.setTitle(readStringElement(parser,"tl"));
