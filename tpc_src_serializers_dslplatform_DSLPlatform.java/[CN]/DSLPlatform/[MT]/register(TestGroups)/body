{
  groups.media.add(new DSLPlatformFullMediaTransformer(),new DSLPlatformFullSerializer(),new SerFeatures(SerFormat.JSON,SerGraph.FLAT_TREE,SerClass.CLASSES_KNOWN,"Serializes all properties with exact names."));
