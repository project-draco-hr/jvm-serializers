{
  List<String> persons=new ArrayList<String>();
  for (  CharSequence p : media.persons) {
    persons.add(p.toString());
  }
  return new data.media.Media(media.uri.toString(),media.title != null ? media.title.toString() : null,media.width,media.height,media.format.toString(),media.duration,media.size,media.bitrate != null ? media.bitrate : 0,media.bitrate != null,persons,reversePlayer(media.player),media.copyright != null ? media.copyright.toString() : null);
}
