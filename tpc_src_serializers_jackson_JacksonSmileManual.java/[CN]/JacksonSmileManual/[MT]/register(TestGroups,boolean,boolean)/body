{
  SmileFactory factory=new SmileFactory();
  factory.configure(SmileGenerator.Feature.CHECK_SHARED_NAMES,sharedNames);
  factory.configure(SmileGenerator.Feature.CHECK_SHARED_STRING_VALUES,sharedValues);
  groups.media.add(JavaBuiltIn.mediaTransformer,new JacksonJsonManual("smile/jackson/manual",factory),new SerFeatures(SerFormat.JSON,SerGraph.FLAT_TREE,SerClass.MANUAL_OPT,""));
}
